{
	"name": "create-tailwindkit-ts",
	"version": "0.3.0",
	"description": "A Sveltekit starter project with Typescript and Latest version of Tailwind pre-configured",
	"bin": "./bin/cli.cjs",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Brisklemonade/sveltekit-typescript-tailwindcss-template"
	},
	"author": "Kamell Perry",
	"license": "MIT",
	"devDependencies": {
		"@rollup/plugin-replace": "^4.0.0",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"autoprefixer": "^10.4.0",
		"dotenv": "^16.0.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"postcss": "^8.4.4",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.6",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"tailwindcss": "^3.0.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^1.35.3"
	}
}
